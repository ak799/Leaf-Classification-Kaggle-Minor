  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "# drop id & species columns. seperate labels in training set\n",
    "labels = train['label']\n",
    "train_df = train.drop(columns=['id','species','label'])"
   ]
  },
